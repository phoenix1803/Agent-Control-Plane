{
    "name": "agent-control-plane",
    "version": "1.0.0",
    "description": "A local-first developer tool for recording, replaying, and testing AI agent behavior",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": {
        "acp": "./dist/cli/index.js"
    },
    "scripts": {
        "build": "tsc",
        "start": "ts-node src/agent/run.ts",
        "run:agent": "ts-node src/agent/run.ts",
        "replay": "ts-node src/agent/replay.ts",
        "inspect": "ts-node src/cli/inspect.ts",
        "test": "ts-node src/cli/test-runner.ts",
        "analyze": "ts-node src/cli/analyze.ts",
        "lint": "eslint src/**/*.ts"
    },
    "keywords": [
        "agent",
        "ai",
        "tracing",
        "replay",
        "testing",
        "debugging"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "chalk": "^4.1.2",
        "commander": "^11.1.0",
        "inquirer": "^8.2.6",
        "yaml": "^2.3.4",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@types/inquirer": "^8.2.10",
        "@types/node": "^20.10.0",
        "@types/uuid": "^9.0.7",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.2"
    }
}