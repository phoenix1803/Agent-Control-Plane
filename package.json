{
    "name": "agent-control-plane",
    "version": "1.0.0",
    "description": "A local-first developer tool for recording, replaying, and testing AI agent behavior",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": {
        "acp": "./dist/cli/index.js"
    },
    "scripts": {
        "build": "tsc",
        "start": "ts-node src/agent/run.ts",
        "run:agent": "ts-node src/agent/run.ts",
        "run:bg-worker": "ts-node src/core/vector-store/bg-worker.ts",
        "replay": "ts-node src/agent/replay.ts",
        "inspect": "ts-node src/cli/inspect.ts",
        "replay:control": "ts-node src/cli/replay-control.ts",
        "test": "ts-node src/cli/test-runner.ts",
        "analyze": "ts-node src/cli/analyze.ts",
        "lint": "eslint src/**/*.ts",
        "ext:build": "cd vscode-extension && npm install && npm run compile",
        "ext:open": "npm run ext:build && code --extensionDevelopmentPath=vscode-extension .",
        "ext:dev": "npm run build && npm run ext:build && code --extensionDevelopmentPath=e:\\Code\\Agent-Control-Plane\\vscode-extension .",
        "ext:quick": "code --extensionDevelopmentPath=e:\\Code\\Agent-Control-Plane\\vscode-extension ."
    },
    "keywords": [
        "agent",
        "ai",
        "tracing",
        "replay",
        "testing",
        "debugging"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@google/generative-ai": "^0.24.1",
        "@huggingface/transformers": "^3.8.1",
        "@langchain/community": "^1.1.15",
        "@langchain/core": "^1.1.24",
        "@langchain/google": "^0.1.0",
        "@langchain/google-genai": "^2.1.18",
        "@langchain/openai": "^1.2.7",
        "@langchain/qdrant": "^1.0.1",
        "@langchain/textsplitters": "^1.0.1",
        "@qdrant/js-client-rest": "^1.16.2",
        "@xenova/transformers": "^2.17.2",
        "bullmq": "^5.69.1",
        "chalk": "^4.1.2",
        "commander": "^11.1.0",
        "dotenv": "^16.4.5",
        "inquirer": "^8.2.6",
        "ioredis": "^5.8.2",
        "langchain": "^1.2.24",
        "mathjs": "^15.1.1",
        "redis-parser": "^3.0.0",
        "uuid": "^9.0.1",
        "yaml": "^2.3.4"
    },
    "devDependencies": {
        "@types/inquirer": "^8.2.10",
        "@types/node": "^20.10.0",
        "@types/uuid": "^9.0.7",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.2"
    }
}
